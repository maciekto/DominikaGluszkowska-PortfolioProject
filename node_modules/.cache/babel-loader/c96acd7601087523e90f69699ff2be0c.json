   this.textAnimate();\n  },\n  methods: {\n    textAnimate() {\n      let prevScrollposN = window.pageYOffset;\n      // eslint-disable-next-line\n      window.addEventListener('scroll', onScroll);\r\n      function elementVisible(el) {\n        const top = el.offsetTop;\n        const height = el.offsetHeight;\n        const bottom = top + height;\n\n        const IsOverBottom = top > (window.pageYOffset + window.innerHeight);\n        const IsBeforeTop = bottom < window.pageYOffset;\n\n        if (!IsOverBottom && !IsBeforeTop) {\n          el.classList.add('show');\n        } else {\n          el.classList.remove('show');\n        }\n      }\n      function onScroll() {\n        // eslint-disable-next-line\n        for (const item of document.querySelectorAll('.Animate')) {\r\n          elementVisible(item);\n        }\n        // eslint-disable-next-line\n        for (const item of document.querySelectorAll('.Animate2')) {\r\n          elementVisible(item);\n        }\n        // eslint-disable-next-line\n        for (const item of document.querySelectorAll('.Animate3')) {\r\n          elementVisible(item);\n        }\n        // eslint-disable-next-line\n        let currentScrollPosN = window.pageYOffset;\r\n        if (prevScrollposN > currentScrollPosN) {\n          document.querySelector('.NavBar').style.cssText = 'top: 0;';\n        } else if (currentScrollPosN > 2200) {\n          document.querySelector('.NavBar').style.cssText = 'top: 0;';\n        } else {\n          document.querySelector('.NavBar').style.cssText = 'top: -10vh;';\n        }\n        prevScrollposN = currentScrollPosN;\n      }\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n  @import '0-plugins/_plugins-dir';\n  @import '1-base/_base-dir';\n  @import '2-layouts/_layouts-dir';\n  @import '3-modules/_modules-dir';\n  @import '4-states/_states-dir';\n  .Animate {\n    transition: 1s;\n    opacity: 0;\n    transform: translateY(200px);\n  }\n  .Animate2 {\n    transition: 1s;\n    o